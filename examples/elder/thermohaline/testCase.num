  2
 $NON_LINEAR_SOLVER  
  ; method error_tolerance max_iterations relaxation
  PICARD LMAX 100 0.0 1.e-3
 $COUPLING_CONTROL
  LMAX 1e-3

#NUMERICS
 $PCS_TYPE
  MASS_TRANSPORT
 $ELE_GAUSS_POINTS
  2
