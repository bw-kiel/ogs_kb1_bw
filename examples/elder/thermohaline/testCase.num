 $NON_LINEAR_SOLVER  
  ; method error_tolerance max_iterations relaxation
  PICARD LMAX 100 0.0 1.
 $COUPLING_CONTROL
  LMAX 1e-1

#NUMERICS
 $PCS_TYPE
  HEAT_TRANSPORT
